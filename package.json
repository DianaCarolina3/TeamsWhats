{
  "name": "TeamsWhats",
  "version": "1.0.0",
  "description": "This is a project where I made a social network called TeamsWhats. Project of social media server, implementing the learning of backend and nodeJS.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/app.js",
    "prepare": "husky install",
    "prettier": "prettier . --write",
    "lint": "eslint . --fix",
    "fix": "npm run prettier && npm run lint",
    "dev": "nodemon src/app.js",
    "dev:inspect": "nodemon --inspect src/app.js",
    "dev:debug": "set DEBUG=express:*,app:* && npm run dev",
    "debug": "set DEBUG=express:*,app:* && npm run start"
  },
  "keywords": [
    "node",
    "javascript",
    "backend",
    "server",
    "social",
    "postgreSQL",
    "express",
    "node",
    "redis",
    "swagger",
    "jsonwebtokens"
  ],
  "author": "Diana Carolina Campuzano Garz√≥n <dcaroli2015@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^2.3.0"
  },
  "dependencies": {
    "axios": "1.12.0",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "9.0.2",
    "path": "^0.12.7",
    "pg": "8.16.3",
    "redis": "5.8.2",
    "swagger-ui-express": "^4.1.6"
  }
}
